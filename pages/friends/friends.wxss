}

.user-avatar {
  border-right: 1px solid #eee;
  width: 100rpx;
}

.user-avatar-img {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.user-name {
  padding-left: 30rpx;
}

