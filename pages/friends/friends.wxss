/*搜索*/
.inp_box{
  position: fixed;
  top: 102rpx;
  left: 0;
  background-color: #fff;
  width: 100%;
  height: 80rpx;
  border-bottom: 2rpx solid #f7f8ff;
  z-index: 1000;
}
.inp_box>view{
  position: relative;
  display: inline-block;
  width: 65%;
  height: 30rpx;
  padding-top: 15rpx;
}
/*放大镜*/
.inp_box>view>image{
  position: absolute;
  top: 25rpx;
  left: 55rpx;
  width: 30rpx;
  height: 30rpx;
}
/*搜索框*/
.inp_box>view>input{
  padding-left: 100rpx;
  width: 100%;
  height: 30rpx;
}
/*搜索按钮*/
.inp_box>button{
  margin-top: 10rpx;
  margin-right: 20rpx;
  background-color: #5e75ff;
  float: right;
}
/*网络状态及个性签名*/
.text_ell:nth-child(2){
  margin-top: 10rpx;
