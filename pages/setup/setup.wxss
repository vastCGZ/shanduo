/* pages/setup/setup.wxss */
page {
  height: 100%;
}

.container {
  align-items: stretch;
}

.bg_body {
  background-color: #f7f7f7;
  height: 100%;
  text-align: center;
}

.bg_body>view {
  background-color: #fff;
  padding-left: 30rpx;
}

.nav {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  height: 100rpx;
  margin-top: 5rpx;
  line-height: 100rpx;
}
.nav:nth-child(3), .nav:nth-child(4) {
  margin-top: 2rpx;
}
.btn{
  display: inline-block;
  width: 80%;
  background-color: #5e75ff;
  border-radius: 50rpx;
  margin-top: 30rpx;
  color: #fff;
}