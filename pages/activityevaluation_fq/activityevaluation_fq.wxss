/* pages/activityevaluation_fq/activityevaluation_fq.wxss */
page{
  background-color: #f7f8ff;
}
.fq_whole{
  padding: 20rpx;
}
.fq_header, .fq_body{
  background-color: #fff;
}
.fq_header{
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid #f7f8ff;
}
.fq_header>view{
  height: 50%;
  text-align: center;
  line-height: 50rpx;
}
.fq_header>view:nth-child(2)>text{
  color: #999;
}
.fq_text_left{
  margin-left: 20rpx;
}
.fq_pjr_li>image:nth-child(1){
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.fq_pjr_li>text{
  vertical-align: middle;
  margin-right: 10rpx;
}
.fq_pjr_li>.pj_aix{  
  height: 23rpx;  
  width: 26rpx;  
  display: inline-block;
  vertical-align: middle;
  margin-right: 5rpx;
}
.fq_cypj_li{
  width: 100%;
  height: 70rpx;
}
.fq_cypj_li>input{
  width: 550rpx;
  height: 50rpx;
  float: right;
  margin-right: 20rpx;
  padding-left: 20rpx;
  line-height: 50rpx;
  background-color: #f7f8ff;
}
.psection{
  font-size: 24rpx;
}
.fq_footer{
  width: 100%;
  height: 96rpx;
}
.fq_footer>view{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 95%;
  height: 96rpx;
  margin: 20rpx;
  color: #fff;
  line-height: 96rpx;
  text-align: center;
  background-color: #5E75FF;
  z-index: 100;
}